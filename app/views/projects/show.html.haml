%p#notice= notice

%p
  %b Nom:
  = @project.name
%p
  %b Titre:
  = @project.title
%p
  %b Description:
  = @project.description

= link_to 'Back', projects_path, :class => "btn btn-primary"
= link_to 'Edit', edit_project_path(@project), :class => "btn btn-primary"
= link_to 'New', new_project_path, :class => "btn btn-primary"
= link_to 'Delete', @project, :confirm => 'Are you sure?', :method => :delete, :class => "btn btn-primary"
